{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "additionalProperties": false,
  "required": ["handle"],
  "properties": {
    "_token": {
      "$id": "#/properties/_token",
      "type": "string",
      "title": "The _token Schema",
      "default": "",
      "examples": [
        "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJ1c2VybmFtZSI6InJvbmkiLCJpYXQiOjE1NDU0MjI0ODd9.-oukEOKh5iIEH650cqh8xn1QFbSkwE9gS3sHEsRRLHg"
      ],
      "minLength": 1,
      "pattern": "^(.*)$"
    },
    "handle": {
      "$id": "#/properties/handle",
      "type": "string",
      "title": "The Handle Schema",
      "default": "",
      "examples": ["apple"],
      "minLength": 1,
      "pattern": "^(.*)$"
    },
    "name": {
      "$id": "#/properties/name",
      "type": "string",
      "title": "The Name Schema",
      "default": "",
      "examples": ["Apple Inc"],
      "minLength": 1,
      "pattern": "^(.*)$"
    },
    "num_employees": {
      "$id": "#/properties/num_employees",
      "type": "integer",
      "title": "The Num_employees Schema",
      "default": 0,
      "examples": [300],
      "minimum": 1.0
    },
    "description": {
      "$id": "#/properties/description",
      "type": "string",
      "title": "The Description Schema",
      "default": "",
      "examples": ["Amazing Cooking"],
      "pattern": "^(.*)$"
    },
    "logo_url": {
      "$id": "#/properties/logo_url",
      "type": "string",
      "title": "The Logo_url Schema",
      "default": "",
      "examples": ["https://www.amazingcooking.com/logo.png"],
      "pattern": "^(.*)$"
    }
  }
}
